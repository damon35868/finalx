{
	"name": "miniprogram",
	"version": "1.0.0",
	"private": true,
	"description": "",
	"templateInfo": {
		"name": "default",
		"typescript": true,
		"css": "sass"
	},
	"scripts": {
		"dev": "npm run build:dev -- --watch",
		"dev:pro": "npm run build:pro -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"build:dev": "cross-env NODE_ENV=development taro build --type weapp",
		"build:pro": "cross-env NODE_ENV=production taro build --type weapp",
		"build:h5": "taro build --type h5",
		"prepare": "husky install",
		"format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "",
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"@finalx/common": "^1.0.8",
		"@finalx/components": "^1.0.8",
		"@finalx/middleware": "^1.0.8",
		"@fower/taro": "^1.82.0",
		"@tarojs/components": "3.6.8",
		"@tarojs/plugin-framework-react": "3.6.8",
		"@tarojs/react": "3.6.8",
		"@tarojs/runtime": "3.6.8",
		"@tarojs/taro": "3.6.8",
		"cross-env": "^7.0.3",
		"husky": "^8.0.3",
		"prettier": "^3.0.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"recoil": "^0.7.7",
		"recoil-nexus": "^0.5.0"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@tarojs/mini-runner": "3.6.8",
		"@tarojs/webpack-runner": "3.6.8",
		"@types/react": "^17.0.2",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"babel-preset-taro": "3.6.8",
		"eslint": "^8.12.0",
		"eslint-config-taro": "3.6.8",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"stylelint": "^14.4.0",
		"typescript": "^4.1.0"
	}
}
